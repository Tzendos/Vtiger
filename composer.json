{
    "name": "tzendos/vtiger",
    "type": "laravel",
    "description": "Package to run Vtiger Webservice API",
    "keywords": [
        "laravel",
        "vtiger"
    ],
    "homepage": "https://github.com/Tzendos/Vtiger",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Pogarsky",
            "email": "hacking.memory@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Tzendos\\Vtiger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tzendos\\Vtiger\\VtigerServiceProvider"
            ],
            "aliases": {
                "Vtiger": "Tzendos\\Vtiger\\Facades\\Vtiger"
            }
        }
    }
}
